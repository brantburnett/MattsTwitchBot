@model MattsTwitchBot.Core.Models.HomePageInfo
@{
    ViewData["Title"] = "Home Page";
    Layout = null;
}

<html>
<head>
    <link rel="stylesheet" href="~/lib/notus/src/notus.css" media="screen" charset="utf-8">
    <link rel="stylesheet" href="~/lib/notus/src/animate.css" media="screen" charset="utf-8">
    <link rel="stylesheet" href="~/css/site.css" media="screen" charset="urf-8" />
</head>
<body>
    <iframe class="bottomRight" id="ytvideo" width="933" height="525"
        src=""
        frameborder="0"
        allow="autoplay;"
        allowfullscreen></iframe>
    
    @if (Model.Badges != null)
    {
        @foreach (var badge in Model.Badges)
        {
            <partial name="_SocialMediaBadge" model="@badge"/>
        }
    }

    <script src="https://kit.fontawesome.com/aa2e9f1ae2.js" crossorigin="anonymous"></script>
    <script src="~/lib/signalr/dist/browser/signalr.js"></script>
    <script src="~/js/hub.js" type="module"></script>
    <script src="~/lib/notus/src/notus.js" type="text/javascript"></script>
    <script src="https://ajax.aspnetcdn.com/ajax/jQuery/jquery-3.4.1.min.js"></script>
    <script src="~/js/home.js" type="module"></script>
</body>
</html>

