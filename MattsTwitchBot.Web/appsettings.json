{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Couchbase": {
    "ConnectionString" : "couchbase://couchbaseLocation", 
    "Servers": [ "http://couchbaselocation:8091" ],
    "Username": "<couchbaseUsername>",
    "Password": "<couchbasePassword>",
    "BucketName": "twitchchat"
  },
  "Twitch": {
    "Username": "<twitch username>",
    "OauthKey": "oauth:<twitch oauthkey>",
    "ApiClientId": "<twitch api client id>",
    "ApiClientSecret": "<twitch api client secret>"
  },
  "BearerToken": "<random characters of your choice>"
}
